{
  "name": "vitalia-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen-api": "ng-openapi-gen --input http://localhost:8088/api/v1/v3/api-docs --output src/app/api && node scripts/patch-api-config.js",
    "lint": "ng lint",
    "storybook": "ng run vitalia-web:storybook",
    "build-storybook": "ng run vitalia-web:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.2.0",
  "dependencies": {
    "@angular/animations": "^21.0.5",
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.1.0-next.0",
    "@angular/compiler": "^21.1.0-next.0",
    "@angular/core": "^21.1.0-next.0",
    "@angular/forms": "^21.1.0-next.0",
    "@angular/material": "^21.0.2",
    "@angular/material-date-fns-adapter": "^21.0.2",
    "@angular/platform-browser": "^21.1.0-next.0",
    "@angular/router": "^21.1.0-next.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@ng-matero/extensions": "^21.0.0",
    "@ngx-formly/core": "^7.0.1",
    "@ngx-formly/material": "^7.0.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "apexcharts": "^5.3.6",
    "date-fns": "^4.1.0",
    "font-awesome": "^4.7.0",
    "ng-apexcharts": "^2.0.4",
    "ngx-scrollbar": "^18.0.0",
    "ngx-toastr": "^19.1.0",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0-next.1",
    "@angular/cli": "^21.1.0-next.1",
    "@angular/compiler-cli": "^21.1.0-next.0",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.47.0",
    "vitest": "^4.0.8",
    "storybook": "^10.2.0",
    "@storybook/angular": "^10.2.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-onboarding": "^10.2.0",
    "@angular-devkit/build-angular": "^21.1.0-next.0",
    "@angular-devkit/architect": "^0.2101.0-next.0",
    "@angular-devkit/core": "^21.1.0-next.0",
    "@angular/platform-browser-dynamic": "^21.1.0-next.0",
    "@compodoc/compodoc": "^1.2.1",
    "eslint-plugin-storybook": "^10.2.0"
  }
}
